{
  "name": "dexscreener-realtime-client",
  "version": "1.0.0",
  "description": "Node.js/TypeScript SDK and CLI for consuming SSE streams from DexScreener Realtime Monitor Apify Actor",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "dexrt": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dexscreener",
    "sse",
    "realtime",
    "crypto",
    "dex",
    "trading",
    "apify"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "eventsource": "^4.1.0",
    "inquirer": "^13.2.1",
    "js-yaml": "^4.1.1",
    "ora": "^9.1.0",
    "prom-client": "^15.1.3",
    "yargs": "^17.7.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/yargs": "^17.0.35",
    "fast-check": "^4.4.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
